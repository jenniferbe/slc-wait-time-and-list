
<li><%= link_to "Back to Student Sign In", new_student_path, :method => :get %></li>
<% if flash[:error] %>
    <div class="notice"><%= flash[:error] %></div>
<% end %>
<%= form_tag(tutor_authenticate_path, method: "post") do %>
    <label>Tutor Password</label>
    <%= password_field_tag "pass"%>
    <%= submit_tag "Submit" %>
<%end%>